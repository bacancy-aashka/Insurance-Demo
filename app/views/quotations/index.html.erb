<p id="notice"><%= notice %></p>

<h1>Quotations</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Eval value</th>
      <th>Address</th>
      <th>Postal code</th>
      <th>City</th>
      <th>Province</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotations.each do |quotation| %>
      <tr>
        <td><%= quotation.first_name %></td>
        <td><%= quotation.last_name %></td>
        <td><%= quotation.email %></td>
        <td><%= quotation.eval_value %></td>
        <td><%= quotation.address %></td>
        <td><%= quotation.postal_code %></td>
        <td><%= quotation.city %></td>
        <td><%= quotation.province %></td>
        <td><%= link_to 'Show', quotation %></td>
        <td><%= link_to 'Edit', edit_quotation_path(quotation) %></td>
        <td><%= link_to 'Destroy', quotation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quotation', new_quotation_path %>
